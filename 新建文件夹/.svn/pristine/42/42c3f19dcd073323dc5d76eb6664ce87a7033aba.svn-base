.btn {
  display: block;
  height:24px;
  line-height: 22px;
  text-align: center;
  font-size:12px;
  font-family: PingFangSC-Light, sans-serif;
  color: #444;
  border-radius: 2px;
  border: 1px solid #cfcfcf;
  cursor: pointer;
}

.btn.btn-default {
  width:60px;
}
.btn.btn-default:hover {
  background:#fafafa;
}
.btn.btn-default:active,
.btn.btn-default.active {
  background:#236ee7;
  border-color:#236ee7;
  color:#FFF;
}

.btn.btn-on {
  border-color: #727F95;
  background:#727F95;
  color:#FFF;
}
.btn.btn-on:hover {
  border-color: #79869C;
  background:#79869C;
}
.btn.btn-on:active {
  border-color: #6A768A;
  background:#6A768A;
}

.btn.btn-off {
  border-color: #E6E6E6;
  background:#FFF;
  color:#80838A;
}
.btn.btn-off:hover {
  background:#FAFAFA;
}
.btn.btn-off:active {
  background:#F2F2F2;
}

.btn.btn-primary {
  background:#247FF4;
  border-color: #247FF4;
  color:#FFF;
}
.btn.btn-primary:hover {
  border-color: #3C8EF8;
  background:#3C8EF8;
}
.btn.btn-primary:active {
  border-color: #236ee7;
  background:#236ee7;
}

.btn.btn-inline {
  display: inline-block;
  height:auto;
  color:#236ee7;
  border:0;
  text-align: left;
}

.btn-pi,
.btn-totop {
  line-height: 24px;
  border:0;
  border-radius: 12px;
  text-decoration: none;
  background: rgba(0, 0, 0, 0.35);
  text-align: center;
  color: #fffefe;
  position: fixed;
  bottom: 10px;
  z-index: 5;
}
.btn-pi {
  padding: 0 6px;
  right: 50px;
}
.btn-totop {
  width: 24px;
  font-size:22px;
  right: 10px;
}
.btn-checkbox {
  padding-left: 24px;
}
.btn-checkbox::before {
  content: "";
  display: block;
  width: 14px;
  height: 14px;
  border: 1px solid #878787;
  border-radius: 2px;
  position: absolute;
  left: 5px;
  top: 50%;
  margin-top: -7px;
}
.btn-checkbox:hover::before {
  border-color: #a2a3a7;
}
.btn-checkbox:active::before {
  border-color: #686669;
}
.btn-checkbox.checked::before {
  border: 0;
  background-color: #247ff4;
}
.btn-checkbox.checked:hover::before {
  background-color: #3c8ef8;
}
.btn-checkbox.checked:active::before {
  background-color: #236ee7;
}

.btn.disabled,
.btn.disabled:hover,
.btn.disabled:active {
  border-color:#CFCFCF;
  color:#CCC;
  background:#EEE;
  cursor: not-allowed;
}

.btn-group {
  position: relative;
  overflow: hidden;
}
.btn-group>.btn {
  float:left;
  display: block;
  border-radius: 0;
  border-left-width: 0;
}
.btn-group>.btn:nth-child(1) {
  border-top-left-radius: 3px;
  border-bottom-left-radius: 3px;
  border-left-width: 1px;
}
.btn-group>.btn:nth-last-child(1) {
  border-top-right-radius: 3px;
  border-bottom-right-radius: 3px;
}


.mask {
  width:100%;
  background:rgba(0, 0, 0, 0.84);
  overflow-y:scroll;
  position: fixed;
  top:0;
  left:0;
  bottom:0;
  z-index: 5;
}


.layout {
  width: 100%;
  user-select: none;
  position: absolute;
  top: 0;
  bottom: 0;
}
.pages {
  width: calc(100% - 181px);
  position: fixed;
  left: 181px;
  top: 45px;
  bottom: 0;
}
.page {
  display: none;
  width:100%;
  height:100%;
}
.page.active {
  display: block;
}
.page-title {
  width: calc(100% - 181px);
  height: 41px;
  border-bottom: 1px solid #e6e6e6;
  padding: 0 14px;
  background: #fff;
  position: fixed;
  top: 45px;
  left: 181px;
  z-index: 5;
}
.page-title > h2 {
  display: inline-block;
  height: 40px;
  line-height: 40px;
  font-size: 13px;
  font-weight: normal;
  font-family: PingFangSC-Semibold, sans-serif;
  color: #3c424e;
  padding: 0 10px;
  position: relative;
}
.page-title > .title-right {
  text-align: center;
  float: right;
}
.page-title > .title-right > * {
  display: block;
  float:right;
}
.page-title > .title-right > app-dropdown {
  margin: 8px 0 0 10px;
}

.page-content {
  width: calc(100% - 181px);
  overflow-y: scroll;
  position: fixed;
  left: 181px;
  top: 86px;
  bottom: 0;
}
.page.notitle .page-content {
  top: 45px;
}
.page.search .page-content {
  margin-top:72px;
}

.page.search .table-head {
  top:117px;
}

.page-content.page-table {
  padding-top:27px;
}

.page-loading {
  width:300px;
  height:80px;
  padding-top:50px;
  line-height: 30px;
  text-align: center;
  font-size:13px;
  color:#80838a;
  position: absolute;
  top:50%;
  left:50%;
  margin-left:-150px;
  margin-top:-40px;
}
.page-loading>.icon {
  top: 0;
  margin-top: 0;
}
.dataStatus {
  clear: both;
  width: 100%;
  height: 60px;
  line-height: 60px;
  text-align: center;
  position: relative;
}
.dataStatus>p {
  display: inline;
  position: relative;
  font-size:12px;
}
.dataStatus>p>.icon {
  width:16px;
  height:16px;
  left:-20px;
  margin-top:-8px;
  margin-left:0;
}


.searchTitle {
  width: calc(100% - 181px);
  height:72px;
  background:#f8f8f8;
  border-bottom:1px solid #eee;
  text-align: center;
  line-height: 24px;
  color:#3c3f44;
  font-size:12px;
  padding:24px;
  position: fixed;
  left: 181px;
  top:45px;
  z-index: 2;
}
.searchTitle>span {
  display: inline-block;
  overflow: hidden;
}
.searchTitle>span.keyword {
  max-width: 220px;
  color:#236ee7;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.searchTitle>span.count {
  margin:0 5px;
}


.dropdown {
  width: auto;
  border: 1px solid #b7b7b7;
  border-radius: 5px;
  background: #fff;
  padding: 7px 0;
  box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.4);
  position: absolute;
  z-index: 3;
}
.dropdown > .btn {
  display: block;
  width: 100%;
  max-width: 300px;
  padding: 0 24px;
  border:0;
  border-radius: 0;
  overflow: hidden;
  text-align: left;
  text-overflow: ellipsis;
  white-space: nowrap;
  position: relative;
}
.dropdown > .btn.active::before {
  content: "";
  display: block;
  width: 24px;
  height: 24px;
  position: absolute;
  left: 0;
  top: 0;
}
.dropdown > .btn .name {
}
.dropdown > .btn svg {
  display: none;
  font-size: 20px;
  color:#FFF;
  position: absolute;
  left:2px;
  top:50%;
  margin-top:-10px;
}
.dropdown.dropdownTop {
  bottom:28px;
}
.dropdown.dropdownRight {
  right:0;
}
.dropdown.dropdownBottom {
  top:28px;
}
.dropdown.dropdownLeft {
  left:0;
}
